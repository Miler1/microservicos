{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport UserService from '../services/user.service';\nimport CourseService from '../services/course.service';\nexport default {\n  name: 'profile-test',\n  data() {\n    return {\n      currentUser: null,\n      transactions: []\n    };\n  },\n  created() {\n    this.currentUser = UserService.currentUserValue;\n    if (!this.currentUser) {\n      this.$router.push('/login');\n    }\n    CourseService.filterTransactions(this.currentUser.id).then(trans => {\n      this.transactions = trans.data;\n    });\n  },\n  methods: {\n    logOut() {\n      UserService.logOut().then(() => {\n        this.$router.push('/login');\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AA6CA;AACA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;MACA;IACA;IACAC;MACA;IACA;EACA;EACAC;IACAC;MACAC;QACA;MACA;IACA;EACA;AACA","names":["name","data","currentUser","transactions","created","CourseService","methods","logOut","UserService"],"sourceRoot":"src/views","sources":["ProfileTest.vue"],"sourcesContent":["<template>\n    <div class=\"container\">\n        <header class=\"jumbotron\">\n            <h1>\n                <strong>{{currentUser.name}}</strong> Welcome to my app.\n            </h1>\n        </header>\n        <a class=\"btn btn-link log-out\" @click=\"logOut\" >LogOut</a>\n\n        <div class=\"transaction-container\">\n            <div class=\"card\">\n                <div class=\"card-header\">\n                    <div class=\"col col-xs-6\">\n                        <h3 class=\"panel-title\">All Transactions</h3>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card-body\">\n                <table class=\"table table-striped\">\n                    <thead>\n                        <tr>\n                            <th scope=\"col\">#</th>\n                            <th scope=\"col\">Title</th>\n                            <th scope=\"col\">Author</th>\n                            <th scope=\"col\">Category</th>\n                            <th scope=\"col\">Date of Issue</th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        <tr v-for=\"(item, index) in transaction\" v-bind:key=\"item.id\">\n                            <th scope=\"row\">{{index}}</th>\n                            <td>{{item.course.title}}</td>\n                            <td>{{item.course.author}}</td>\n                            <td>{{item.course.category}}</td>\n                            <td>{{item.dateOfIssue}}</td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n\n        </div>\n    </div>\n</template>\n<script>\nimport UserService from '../services/user.service';\nimport CourseService from '../services/course.service';\nexport default {\n    name: 'profile-test',\n    data() {\n        return {\n            currentUser: null,\n            transactions: []\n        };\n    },\n    created() {\n        this.currentUser = UserService.currentUserValue;\n        if(!this.currentUser){\n            this.$router.push('/login');\n        }\n        CourseService.filterTransactions(this.currentUser.id).then(trans => {\n            this.transactions = trans.data;\n        });\n    },\n    methods: {\n        logOut() {\n            UserService.logOut().then(() => {\n                this.$router.push('/login');\n            });\n        }\n    }\n}\n</script>\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}